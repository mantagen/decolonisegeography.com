    "gatsby-link": "^5.12.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-twitter-embed": "^4.0.4",
    "react-use": "^17.2.3",
    "styled-components": "^5.2.1",
    "throttle-debounce": "^3.0.1"
